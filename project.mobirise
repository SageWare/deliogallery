        {
          "_name": "aform1",
          "_sourceTheme": "expertm5",
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "overflow": "hidden",
            ".mbr-overlay": {
              "background-color": "@overlayColor",
              "opacity": "@overlayOpacity"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            ".container": {
              "padding-left": "10px",
              "padding-right": "10px"
            },
            ".row": {
              "margin-left": "-10px",
              "margin-right": "-10px",
              "flex-wrap": "wrap",
              "align-items": "stretch",
              "& when (@reverseContent)": {
                "flex-direction": "row-reverse"
              },
              "> [class*=\"col\"]": {
                "padding-left": "10px",
                "padding-right": "10px"
              }
            },
            ".col-info-card": {
              "@media (max-width: 991px)": {
                "margin-bottom": "50px"
              }
            },
            ".mbr-section-head": {
              "width": "100%",
              "padding-bottom": "40px"
            },
            ".mbr-section-title": {
              "color": "#5bb4ed",
              "@media (max-width: 767px)": {
                "text-align": "center"
              }
            },
            ".mbr-section-subtitle": {
              "color": "#C4CFDE",
              "margin-top": "15px",
              "@media (max-width: 767px)": {
                "margin-top": "10px",
                "text-align": "center"
              }
            },
            ".item-card": {
              "height": "100%"
            },
            ".item-wrap": {
              "position": "relative",
              "height": "100%",
              "width": "100%",
              "padding": "30px",
              "border-radius": "10px",
              "overflow": "hidden",
              "& when (@showCardBg)": {
                "background": "@cardBg"
              },
              "& when (@cardShadow)": {
                "box-shadow": "10px 10px 19px @shadowColor1, -10px -10px 19px @shadowColor2"
              },
              "@media (max-width: 767px)": {
                "padding": "50px 20px"
              },
              "&:hover": {
                ".item-img": {
                  "img": {
                    "transform": "scale(1.1)"
                  }
                }
              }
            },
            ".item-img": {
              "display": "flex",
              "align-items": "center",
              "justify-content": "center",
              "width": "100%",
              "border-radius": "10px",
              "overflow": "hidden",
              "img": {
                "width": "100%",
                "aspect-ratio": "(115 / 72)",
                "object-fit": "cover",
                "transition": "0.4s all"
              }
            },
            ".card-box": {
              "padding-top": "28px"
            },
            ".card-title": {
              "color": "#000000",
              "margin-bottom": "9px",
              "text-align": "left"
            },
            ".card-subtitle": {
              "color": "#878e99",
              "margin-bottom": "15px"
            },
            ".card-text": {
              "color": "#000000",
              "margin-bottom": "20px"
            },
            ".card-contacts": {
              "color": "#ffffff",
              "a": {
                "position": "relative",
                "transition": "0.4s all",
                "&:before": {
                  "content": "\"\"",
                  "position": "absolute",
                  "bottom": "0",
                  "left": "0",
                  "width": "0",
                  "height": "1px",
                  "background-color": "@primaryColor",
                  "transition": "0.4s all"
                },
                "&:hover": {
                  "color": "@primaryColor !important",
                  "&:before": {
                    "width": "100%"
                  }
                }
              }
            },
            ".social-container": {
              "width": "100%",
              "padding-top": "30px",
              "@media (max-width: 991px)": {}
            },
            ".social-title": {
              "color": "#000000",
              "margin-bottom": "8px"
            },
            ".social-list": {
              "display": "flex",
              "align-items": "center",
              "flex-wrap": "wrap",
              "margin": "-15px",
              "margin-top": "24px"
            },
            ".soc-item": {
              "display": "flex",
              "align-items": "center",
              "justify-content": "center",
              "position": "relative",
              "margin": "15px",
              "min-width": "60px",
              "padding": "10px",
              "aspect-ratio": "(1 / 1)",
              "border-radius": "6px",
              "background": "linear-gradient(145deg, @socialBgColor1, @socialBgColor2)",
              "& when (@socialShadow)": {
                "box-shadow": "10px 10px 19px @socialShadow1, -10px -10px 19px @socialShadow2"
              },
              "transition": "0.4s all",
              "&:before": {
                "content": "\"\"",
                "position": "absolute",
                "top": "0",
                "left": "0",
                "z-index": "4",
                "transition": "0.4s all",
                "width": "100%",
                "height": "100%",
                "border-radius": "5px",
                "background": "linear-gradient(to right bottom, #212428, #16181c)",
                "opacity": "0"
              },
              ".mbr-iconfont": {
                "position": "relative",
                "z-index": "5",
                "transition": "0.4s all",
                "font-size": "22px",
                "color": "@socialColor"
              },
              "&:hover": {
                "transform": "translateY(-3px)",
                ".mbr-iconfont": {
                  "color": "@primaryColor"
                },
                "&:before": {
                  "opacity": "1"
                }
              }
            },
            ".form-main-container": {
              "width": "100%",
              "height": "100%",
              "@media (min-width: 1200px)": {
                "& when not (@reverseContent)": {
                  "padding-left": "20px"
                },
                "& when (@reverseContent)": {
                  "padding-right": "20px"
                }
              }
            },
            ".form-wrap": {
              "width": "100%",
              "height": "100%",
              "padding": "30px",
              "border-radius": "10px",
              "& when (@showFormBg)": {
                "background": "@formBg"
              },
              "& when (@formShadow)": {
                "box-shadow": "10px 10px 19px @formShadowColor1, -10px -10px 19px @formShadowColor2"
              },
              "@media (max-width: 575px)": {
                "padding": "20px"
              }
            },
            "form": {
              "width": "100%",
              ".dragArea.row": {
                "justify-content": "flex-start",
                "width": "auto !important"
              },
              "p": {
                "color": "#6C7780"
              },
              ".mbr-section-btn": {
                "text-align": "center",
                "justify-content": "center",
                ".btn": {
                  "width": "100%"
                }
              },
              ".form-group": {
                "margin-bottom": "20px !important"
              },
              ".form-control": {
                "background-color": "#D4D8D9",
                "border-radius": "6px !important",
                "height": "55px",
                "box-shadow": "1px 4px 2px -3px rgba(0, 0, 0, 0.7) inset, -1px -3px 3px -2px rgba(255, 255, 255, 0.2) inset !important",
                "border": "2px solid #191b1e !important",
                "padding": "10px 15px",
                "font-size": "18px",
                "margin-bottom": "0",
                "color": "#6C7780",
                "font-weight": "400",
                "transition": "0.4s all !important",
                "line-height": "29px !important",
                "&::-webkit-input-placeholder": {
                  "color": "#6C7780"
                },
                "&::-moz-placeholder": {
                  "color": "#6C7780"
                },
                "&:-moz-placeholder": {
                  "color": "#6C7780"
                },
                "&:-ms-input-placeholder": {
                  "color": "#6C7780"
                }
              },
              "select": {
                "color": "#6C7780 !important"
              },
              "textarea": {
                "height": "216px !important",
                "resize": "none"
              },
              "input, textarea": {
                "&:active, &:focus": {
                  "border-color": "@primaryColor !important",
                  "box-shadow": "none !important",
                  "color": "#6C7780 !important",
                  "outline": "none !important",
                  "&::-webkit-input-placeholder": {
                    "color": "#6C7780"
                  },
                  "&::-moz-placeholder": {
                    "color": "#6C7780"
                  },
                  "&:-moz-placeholder": {
                    "color": "#6C7780"
                  },
                  "&:-ms-input-placeholder": {
                    "color": "#6C7780"
                  }
                }
              },
              ".row": {
                "margin-left": "-15px !important",
                "margin-right": "-15px !important"
              },
              ".row [class*=col]": {
                "padding-left": "15px !important",
                "padding-right": "15px !important"
              },
              "label": {
                "width": "100%",
                "color": "#6C7780",
                "margin-bottom": "14px"
              },
              ".form-control-label": {
                "width": "100%"
              },
              ".form-check-label": {
                "margin-bottom": "14px"
              },
              ".form-check-input": {
                "border-color": "#6C7780 !important",
                "outline": "none !important",
                "box-shadow": "none !important",
                "background-color": "transparent !important",
                "&:focus, &:hover": {
                  "border-color": "@primaryColor !important"
                }
              },
              ".form-check-input:checked": {
                "border-color": "@primaryColor !important",
                "background-color": "#6C7780 !important"
              }
            },
            "LABEL": {
              "color": "#ffbc00"
            }
          },
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"form1 expertm5\" group=\"Forms\" plugins=\"formstyler, datepicker\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"2\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <select title=\"Align\" name=\"contentAlign\">\n            <option value=\"justify-content-start\">Left</option>\n            <option value=\"justify-content-center\" selected>Center</option>\n            <option value=\"justify-content-end\">Right</option>\n        </select>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showMainTitle\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showMainSubtitle\">\n        <input type=\"checkbox\" title=\"Info Card\" name=\"showCard\" checked>\n        <header condition=\"showCard\">Card</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" condition=\"showCard\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" condition=\"showCard\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" condition=\"showCard\" checked>\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" condition=\"showCard\">\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" condition=\"showCard\" checked>\n        <input type=\"checkbox\" title=\"Social Title\" name=\"showSocialTitle\" condition=\"showSocial && showCard\" checked>\n        <input type=\"color\" title=\"Social Color\" name=\"socialColor\" value=\"#000000\" condition=\"showSocial && showCard\">\n        <input type=\"color\" title=\"Social BG Color 1\" name=\"socialBgColor1\" value=\"#bbbbbb\" condition=\"showSocial && showCard\">\n        <input type=\"color\" title=\"Social BG Color 2\" name=\"socialBgColor2\" value=\"#d8d8d8\" condition=\"showSocial && showCard\">\n        <input type=\"checkbox\" name=\"socialShadow\" title=\"Social Shadow\" condition=\"showSocial && showCard\">\n        <input type=\"color\" title=\"Shadow Color 1\" name=\"socialShadow1\" value=\"#000000\" condition=\"socialShadow && showSocial && showCard\">\n        <input type=\"color\" title=\"Shadow Color 2\" name=\"socialShadow2\" value=\"#92b1c0\" condition=\"socialShadow && showSocial && showCard\">\n        <input type=\"range\" name=\"socialCount\" title=\"Social Count\" min=\"1\" max=\"6\" step=\"1\" value=\"4\" condition=\"showSocial && showCard\">\n        <header condition=\"showCard\">Card Style</header>\n        <input type=\"checkbox\" title=\"Card BG\" name=\"showCardBg\" condition=\"showCard\" checked>\n        <input type=\"color\" title=\"Card BG Color\" name=\"cardBg\" value=\"#ffffff\" condition=\"showCard && showCardBg\">\n        <input type=\"checkbox\" name=\"cardShadow\" title=\"Card Shadow\" condition=\"showCard\" checked>\n        <input type=\"color\" title=\"Shadow Color 1\" name=\"shadowColor1\" value=\"#c1c1c1\" condition=\"cardShadow && showCard\">\n        <input type=\"color\" title=\"Shadow Color 2\" name=\"shadowColor2\" value=\"#c1c1c1\" condition=\"cardShadow && showCard\">\n        <header>Form Style</header>\n        <input type=\"checkbox\" title=\"Form BG\" name=\"showFormBg\">\n        <input type=\"color\" title=\"Form BG Color\" name=\"formBg\" value=\"#090f10\" condition=\"showFormBg\">\n        <input type=\"checkbox\" name=\"formShadow\" title=\"Form Shadow\" checked>\n        <input type=\"color\" title=\"Shadow Color 1\" name=\"formShadowColor1\" value=\"#879a9f\" condition=\"formShadow\">\n        <input type=\"color\" title=\"Shadow Color 2\" name=\"formShadowColor2\" value=\"#bbbbbb\" condition=\"formShadow\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background4-2.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type!='color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#212428\" condition=\"bg.type!='color' && overlay\">\n        <input type=\"range\" inline title=\"Overlay Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"bg.type!='color' && overlay\">\n    <input type=\"hidden\" name=\"usedRecaptcha\" value=\"false\"></mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay && bg.type=='image')\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row\">\n            <div class=\"col-12\" mbr-if=\"showMainTitle || showMainSubtitle\">\n                <div class=\"mbr-section-head\">\n                    <h3 class=\"mbr-section-title mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showMainTitle\" data-app-selector=\".mbr-section-title\"><b>CONTACT</b></h3>\n                    <h5 class=\"mbr-section-subtitle mbr-fonts-style align-center mb-0\" mbr-theme-style=\"display-5\" mbr-if=\"showMainSubtitle\" data-app-selector=\".mbr-section-subtitle\"></h5>\n                </div>\n            </div>\n        </div>\n\n        <div class=\"row {{contentAlign}}\">\n            <div class=\"col-12 col-lg-4 col-info-card\" mbr-if=\"showCard\">\n                <div class=\"item-card\">\n                    <div class=\"item-wrap\">\n                        <div class=\"card-bg\"></div>\n                        <div class=\"item-img\">\n                            <img src=\"@PROJECT_PATH@/assets/images/bookcase-with-logo-1.jpg\" alt=\"Delio Enterprises Lobby\">\n                        </div>\n                        <div class=\"card-box\" mbr-if=\"showTitle || showSubtitle || showText || showContacts || showSocial\">\n                            <h5 class=\"card-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showTitle\" data-app-selector=\".card-title\">\n                               <b>Delio Gallery</b>&nbsp;is a venture of <a href=\"https://delioenterprises.com\" class=\"text-secondary\" target=\"_blank\"><b>Delio Enterprises</b></a>. Submissions via this contact form are sent to Delio Enterprises. You should expect a response from contact(at)delioenterprises(.com) and be sure to check your spam folder.<br></h5>\n                            <h6 class=\"card-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".card-subtitle\"></h6>\n                            <p class=\"card-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".card-text\"><br><b>Hours&nbsp;</b>-<b>&nbsp;Eastern Standard Time (EST):</b><br>Monday: 9am - 5pm\n<br>Tuesday: 9am - 5pm\n<br>Wednesday: 9am - 5pm\n<br>Thursday: 9am - 5pm\n<br>Friday: 9am - 3pm\n<br><br>Closed weekends and all major holidays.<br></p>\n                            <p class=\"card-contacts mbr-fonts-style mb-0\" mbr-theme-style=\"display-7\" mbr-if=\"showContacts\" data-app-selector=\".card-contacts\"></p>\n                            <div class=\"social-container\" mbr-if=\"showSocial\">\n                                <p class=\"social-title mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSocialTitle\" data-app-selector=\".social-title\"><b>\n                                    CONNECT WITH GABBY &amp; MIN:</b></p>\n                                <div class=\"social-list\">\n                                    <div class=\"soc-item\">\n                                        <a href=\"https://www.facebook.com/deliogallery\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-facebook socicon\" style></span></a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                                        <a href=\"https://www.instagram.com/deliogallery/\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-instagram socicon\" style></span></a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                                        <a href=\"https://twitter.com/deliogallery\" target=\"_blank\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-twitter socicon\" style></span></a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>3\">\n                                        <a href=\"https://www.linkedin.com/company/deliogallery/\"><span mbr-icon class=\"mbr-iconfont mbr-iconfont-social socicon-linkedin socicon\"></span></a>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>4\">\n                                        <span mbr-icon class=\"socicon-behance socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                    </div>\n                                    <div class=\"soc-item\" mbr-if=\"socialCount>5\">\n                                        <span mbr-icon class=\"socicon-vimeo socicon mbr-iconfont mbr-iconfont-social\"></span>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n\n            <div class=\"col-12 col-lg-8 col-form\">\n                <di